@model EDR.Models.ViewModels.Members

<table>
    <tr>
        <th>
        </th>
        <th>
            Name
        </th>
        <th>
            Admin
        </th>
    </tr>
    @*@Html.EditorFor(Model)*@
    @for (int i = 0; i < Model.MemberList.Count; i++)
    {
        <tr>
            @Html.HiddenFor(model => model.MemberList[i].Id)
            <td>
                @Html.EditorFor(model => model.MemberList[i].User.FirstName)
            </td>
            <td>
                @Html.EditorFor(model => model.MemberList[i].Admin)
            </td>
            <td>
                Remove
            </td>
        </tr>
    }
</table>